CSP_FORM_ACTION='self'
CSP_SANDBOX=
CSP_REPORT_URI=
CSP_REPORT_TO=
CSP_MANIFEST_SRC='self'
CSP_WORKER_SRC='self'
CSP_UPGRADE_INSECURE_REQUESTS=1
CSP_REQUIRE_TRUSTED_TYPES_FOR='script'
CSP_TRUSTED_TYPES='default'

CSP_REPORT_ONLY_EXCLUDE_URL_PREFIXES=
CSP_REPORT_ONLY_DEFAULT_SRC='none'
CSP_REPORT_ONLY_SCRIPT_SRC='self'
CSP_REPORT_ONLY_SCRIPT_SRC_ATTR='none'
CSP_REPORT_ONLY_SCRIPT_SRC_ELEM=
