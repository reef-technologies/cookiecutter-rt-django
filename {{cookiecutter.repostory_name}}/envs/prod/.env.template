ENV=backend-prod
DEBUG=off
DEBUG_TOOLBAR=off
SECRET_KEY=

POSTGRES_DB={{cookiecutter.django_project_name}}
POSTGRES_HOST=db
POSTGRES_PORT=5432
POSTGRES_USER={{cookiecutter.postgres_user}}
POSTGRES_PASSWORD=
DATABASE_POOL_URL=
# using transaction-based db connection pool as DATABASE_URL instead of DATABASE_POOL_URL will break production
DATABASE_URL=postgres://{{cookiecutter.postgres_user}}:@db:5432/{{cookiecutter.django_project_name}}

NGINX_HOST=

CORS_ENABLED=on
CORS_ALLOWED_ORIGINS=
CORS_ALLOWED_ORIGIN_REGEXES=
CORS_ALLOW_ALL_ORIGINS=0

{% if cookiecutter.csp_enabled %}
CSP_EXCLUDE_URL_PREFIXES=
CSP_DEFAULT_SRC='none'
CSP_SCRIPT_SRC='self'
CSP_SCRIPT_SRC_ATTR='none'
CSP_SCRIPT_SRC_ELEM=
CSP_IMG_SRC='self' data:
CSP_OBJECT_SRC='none'
CSP_MEDIA_SRC='self'
CSP_FRAME_SRC='self'
CSP_FONT_SRC='self'
CSP_CONNECT_SRC='self'
CSP_STYLE_SRC='self'
CSP_STYLE_SRC_ATTR='self'
CSP_STYLE_SRC_ELEM=
