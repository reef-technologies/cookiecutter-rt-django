CSP_REPORT_ONLY_OBJECT_SRC='none'
CSP_REPORT_ONLY_MEDIA_SRC='self'
CSP_REPORT_ONLY_FRAME_SRC='self'
CSP_REPORT_ONLY_FONT_SRC='self'
CSP_REPORT_ONLY_CONNECT_SRC='self'
CSP_REPORT_ONLY_STYLE_SRC='self'
CSP_REPORT_ONLY_STYLE_SRC_ATTR='self'
CSP_REPORT_ONLY_STYLE_SRC_ELEM=
CSP_REPORT_ONLY_BASE_URI='self'
CSP_REPORT_ONLY_FRAME_ANCESTORS='none'
CSP_REPORT_ONLY_FORM_ACTION='self'
CSP_REPORT_ONLY_SANDBOX=
CSP_REPORT_ONLY_REPORT_URI=
CSP_REPORT_ONLY_REPORT_TO=
CSP_REPORT_ONLY_MANIFEST_SRC='self'
CSP_REPORT_ONLY_WORKER_SRC='self'
CSP_REPORT_ONLY_UPGRADE_INSECURE_REQUESTS=1
CSP_REPORT_ONLY_REQUIRE_TRUSTED_TYPES_FOR='script'
CSP_REPORT_ONLY_TRUSTED_TYPES='default'
{% endif %}

REDIS_HOST=redis
REDIS_PORT=6379

{% if cookiecutter.use_celery %}
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_TASK_ALWAYS_EAGER=0
CELERY_MASTER_CONCURRENCY=2
CELERY_WORKER_CONCURRENCY=2
{% if cookiecutter.use_flower %}
CELERY_FLOWER_USER=flower
CELERY_FLOWER_PASSWORD=
{% endif %}
{% endif %}

{% if cookiecutter.use_channels %}
CHANNELS_BACKEND_URL=redis://redis:6379/1
{% endif %}

{% if cookiecutter.log_aggregating %}
LOKI_URL={{cookiecutter.log_aggregator_url}}
LOKI_REFRESH_INTERVAL=5s
LOKI_USER=
LOKI_PASSWORD=
LOKI_CLIENT=
LOKI_CLIENT_SERVER_GROUP=
{% endif %}

EMAIL_BACKEND=django.core.mail.backends.smtp.EmailBackend
